{
  "name": "web_name",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "pnpm clear && pnpm i && cross-env ANALYZE=true APP_ENV=production NODE_OPTIONS=--max_old_space_size=4096 PORT=8080 next build",
    "dev": "cross-env NODE_ENV=development next dev -p 3001",
    "dev:nocache": "pnpm clear && pnpm i && pnpm dev",
    "build:dev": "pnpm build:staging",
    "build:staging": "cross-env NODE_ENV=test next build",
    "build:nocache": "pnpm clear && pnpm i && cross-env APP_ENV=production NODE_OPTIONS=--  =4096 PORT=8080 next build",
    "build": "cross-env BUILD_MODE=$BUILD_MODE node set-node-env.js",
    "start": "next start -p 3002",
    "clear": "rm -rf .next && rm -rf node_modules",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{js,ts,tsx,css}\"",
    "create-lang": "plop"
  },
  "dependencies": {
    "@bufbuild/connect-node": "^0.13.0",
    "@bufbuild/connect-web": "^0.13.0",
    "@bufbuild/protobuf": "^1.3.1",
    "@connectrpc/connect": "1.0.0-rc1",
    "@connectrpc/connect-query": "^0.4.4",
    "@connectrpc/connect-web": "^1.0.0",
    "@hookform/resolvers": "^2.8.0",
    "@media/osha-connect": "^1.7.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "autoprefixer": "^10.4.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "cookies-next": "^4.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "google-protobuf": "^3.20.1",
    "i18next": "^23.10.0",
    "jotai": "2.8.0",
    "next": "14.1.1",
    "next-intl": "^3.9.4",
    "next-seo": "^4.28.1",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.5",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-i18next": "^14.0.5",
    "react-modal": "^3.16.1",
    "react-query": "^3.34.19",
    "react-select": "^5.8.0",
    "sass": "^1.62.0",
    "sharp": "^0.32.6",
    "sonner": "^0.6.2",
    "swiper": "^10.2.0",
    "tailwind-merge": "^1.12.0",
    "tailwind-variants": "^0.1.13",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "uuid": "^10.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.7",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-lazyload": "^3.2.1",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "next-transpile-modules": "^10.0.1",
    "plop": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "typescript": "^5"
  },
  "browser": {
    "fs": false,
    "request": false,
    "@bufbuild/connect-node": false
  }
}
